<div class="zone-panel-wrapper tab-pane" id="zone-panel-<%= id %>" data-id="<%= id %>">
  <div class="zone-panel">
    <div class="zone-label">
      Zone <%= id %>
    </div>
    <div class="volume-control">
        <button class="btn volume-up"><%= fa_icon 'chevron-up' %></button>
        <div class="zone-volume" type="number" />0</div>
        <button class="btn volume-down"><%= fa_icon "chevron-down" %></button>
    </div>
    <div class='btn-group zone-enable-control' data-toggle="buttons-radio">
      <button class="btn enabled-true">On</button>
      <button class="btn enabled-false">Off</button>
    </div>
    <div>
  <!--    <button type="button" class="btn muted" data-toggle="button">Mute</button> -->
    </div>
  </div>
</div>
